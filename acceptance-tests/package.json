{
  "name": "acceptance-tests",
  "version": "1.0.0",
  "description": "ui tests experiments",
  "main": "index.js",
  "author": "Thien Tan",
  "config": {
    "file": "Protractor/mynotes.protractor.conf.local.js",
    "chromeVersion": "",
    "chromeVersionPrefix": ""
  },
  "scripts": {
    "setChromeVersionForCI": "npm config set acceptance-tests:chromeVersion $(node chromeVersion.js) && npm config set acceptance-tests:chromeVersionPrefix versions.chrome",
    "webDriverStart": "./node_modules/.bin/cross-var ./node_modules/.bin/webdriver-manager start --detach --$npm_package_config_chromeVersionPrefix $npm_package_config_chromeVersion",
    "webDriverUpdate": "./node_modules/.bin/cross-var ./node_modules/.bin/webdriver-manager update --chrome --$npm_package_config_chromeVersionPrefix $npm_package_config_chromeVersion",
    "webDriverClean": "./node_modules/.bin/webdriver-manager clean",
    "seleniumKill": "./node_modules/.bin/fkill :4444 -s",
    "clean": "./node_modules/.bin/rimraf rmdir build",
    "compileTs": "./node_modules/.bin/tsc -p .",
    "compile": "npm run clean && npm run compileTs",
    "pretest": "npm run compile && npm run seleniumKill && npm run webDriverUpdate && npm run webDriverStart",
    "test": "./node_modules/.bin/cross-var ./node_modules/.bin/protractor $npm_package_config_file",
    "posttest": "npm run seleniumKill",
    "debugPrepare": "npm run pretest && npm run webDriverStart",
    "createReport": "cat Protractor/report/merged-output.json | ./node_modules/.bin/cucumber-junit-enhance > Protractor/cucumber_report.xml"
  },
  "dependencies": {
    "@types/chai": "^4.2.11",
    "@types/cucumber": "^4.0.7",
    "@types/jquery": "^3.5.0",
    "@types/node": "^12.12.50",
    "@types/uuid": "^3.4.9",
    "axios": "^0.19.0",
    "chai": "^4.2.0",
    "cross-var": "^1.1.0",
    "cucumber": "^5.0.2",
    "cucumber-junit": "^1.7.1",
    "find-chrome-version": "^1.0.2",
    "fkill-cli": "^5.2.0",
    "jquery": "^3.5.1",
    "linq": "^3.2.2",
    "protractor": "^5.4.4",
    "protractor-cucumber-framework": "^6.2.1",
    "protractor-cucumber-junit": "^1.1.3",
    "protractor-multiple-cucumber-html-reporter-plugin": "^1.8.1",
    "protractor-react-selector": "^2.2.3",
    "react-scripts": "^3.4.1",
    "rimraf": "^2.7.1",
    "tslib": "^1.13.0",
    "tslint": "^5.20.0",
    "typescript": "^3.9.6",
    "typescript-tslint-plugin": "^0.5.4",
    "unique-names-generator": "^4.3.0",
    "uuid": "^3.3.3",
    "webdriver-manager": "^12.1.7"
  },
  "devDependencies": {
    "protractor-testability-plugin": "^2.0.1"
  }
}
